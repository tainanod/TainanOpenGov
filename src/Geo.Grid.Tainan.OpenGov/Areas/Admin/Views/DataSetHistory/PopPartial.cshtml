@model Geo.Grid.Tainan.OpenGov.Entity.ViewModel.Admin.DataSetHistoryVm
@{
    Layout = "~/Areas/Admin/Views/Shared/_LayoutClean.cshtml";
}
<div i class="panel">
    <div class="panel-body">
        <div id="popMd">
            <textarea id="popMdText">@Model.Contents</textarea>
        </div>
    </div>
</div>

@section scripts
{
    <link href="@Url.Content("~/Areas/Admin/Content/editorMd/css/editormd.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Areas/Admin/Content/editorMd/js/editormd.min.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/Content/editorMd/lib/marked.min.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/Content/editorMd/lib/prettify.min.js")"></script>
    <script type="text/javascript">
        $(function () {
            var testEditormdView2 = editormd.markdownToHTML("popMd", {
                htmlDecode: "style,script,iframe",
                emoji: true,
                taskList: true,
                tex: true,
                flowChart: false,
                sequenceDiagram: false,
            });
        });
    </script>
}